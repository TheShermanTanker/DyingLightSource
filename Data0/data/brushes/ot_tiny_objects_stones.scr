sub main()
{
  brush("ot_tiny_objects_stones")
  {
    brush_params()
    {
      image("roundsoft.png");
      image_rotation(0);
      extent_size(1.0000);
      extent_min(0.0000);
      extent_max(100.0000);
      power(1.0000);
      power_min(0.0000);
      power_max(1.0000);
      fade_distance(0.0000);
      fade_min(0.0000);
      fade_max(100.0000);
      mask("");
    }
    functionality("Edit tiny objects")
    {
      func_params()
      {
        attrib( "m_DensityMapMode[0]", "NoChange" );
        attrib( "m_DensityMapMode[1]", "NoChange" );
        attrib( "m_DensityMapMode[2]", "Set" );
        attrib( "m_DensityMapMode[3]", "NoChange" );
        attrib( "m_DensityMapMode[4]", "NoChange" );
        attrib( "m_DensityMapMode[5]", "NoChange" );
        attrib( "m_DensityMapMode[6]", "NoChange" );
      }
      brush_params()
      {
        extent_size(3.0000);
        extent_min(0.0000);
        extent_max(100.0000);
        power_min(0.0000);
        power_max(1.0000);
        fade_min(0.0000);
        fade_max(100.0000);
      }
    }
  }
}