sub air_conditioning_rotation_continous()
{
	Preset("air_conditioning_rotation_continous")
	{
      OnLevelStart()
	    {
         StopAnimation(0);
   		}
   	  OnEnable()
	    {
         PlayAnimation(0, "conditioner rotation", -1);
   		}
   		OnDisable()
	    {
         StopAnimation(0);
   		}
      
		TimedEvent(0.0)
       { 
       }
	}
}
