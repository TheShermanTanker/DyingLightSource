// steam_emitter_continous
//
// 0 - Steam Emitter

sub steam_emitter_continous()
{
	Preset("steam_emitter_continous")
	{
      OnLevelStart()
	    {
        Enable(0, false);
   		}
   	  OnEnable()
	    {
        Enable(0, false);
   		}
      OnDisable()
        {
        Enable(0, false);
   		}
	  TimedEvent(0.0)
        {
		Enable(3, true);
        }
	  TimedEvent(3.0)
        {
		Enable(6, false);
        }
      GoToBegin(9.01)
        {
        }
	}
}
