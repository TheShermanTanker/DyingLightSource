import "keyboard_localization.scr"

sub main()
{
    use LocalizedKeys();

    //HOWTO: Key(int_keycode, "&localization_id&");  keycode is taken form EKey
    //example: Key(17, "&new_W&");  Key 'W' will be displayed as [&new_W&] instead of [W]

    Key(2,   "1");   // #define DIK_1               0x02
    Key(3,   "2");   // #define DIK_2               0x03
    Key(4,   "3");   // #define DIK_3               0x04
    Key(5,   "4");   // #define DIK_4               0x05
    Key(6,   "5");   // #define DIK_5               0x06
    Key(7,   "6");   // #define DIK_6               0x07
    Key(8,   "7");   // #define DIK_7               0x08
    Key(9,   "8");   // #define DIK_8               0x09
    Key(10,  "9");   // #define DIK_9               0x0A
    Key(11,  "0");   // #define DIK_0               0x0B
    Key(12,  "-");   // #define DIK_MINUS           0x0C    /* - on main keyboard */
    Key(13,  "=");   // #define DIK_EQUALS          0x0D
    Key(16,  "Q");   // #define DIK_Q               0x10
    Key(17,  "W");   // #define DIK_W               0x11
    Key(18,  "E");   // #define DIK_E               0x12
    Key(19,  "R");   // #define DIK_R               0x13
    Key(20,  "T");   // #define DIK_T               0x14
    Key(21,  "Y");   // #define DIK_Y               0x15
    Key(22,  "U");   // #define DIK_U               0x16
    Key(23,  "I");   // #define DIK_I               0x17
    Key(24,  "O");   // #define DIK_O               0x18
    Key(25,  "P");   // #define DIK_P               0x19
    Key(26,  "[");   // #define DIK_LBRACKET        0x1A
    Key(27,  "]");   // #define DIK_RBRACKET        0x1B
    Key(30,  "A");   // #define DIK_A               0x1E
    Key(31,  "S");   // #define DIK_S               0x1F
    Key(32,  "D");   // #define DIK_D               0x20
    Key(33,  "F");   // #define DIK_F               0x21
    Key(34,  "G");   // #define DIK_G               0x22
    Key(35,  "H");   // #define DIK_H               0x23
    Key(36,  "J");   // #define DIK_J               0x24
    Key(37,  "K");   // #define DIK_K               0x25
    Key(38,  "L");   // #define DIK_L               0x26
    Key(39,  ";");   // #define DIK_SEMICOLON       0x27
    Key(40,  "'");   // #define DIK_APOSTROPHE      0x28
    Key(44,  "Z");   // #define DIK_Z               0x2C
    Key(45,  "X");   // #define DIK_X               0x2D
    Key(46,  "C");   // #define DIK_C               0x2E
    Key(47,  "V");   // #define DIK_V               0x2F
    Key(48,  "B");   // #define DIK_B               0x30
    Key(49,  "N");   // #define DIK_N               0x31
    Key(50,  "M");   // #define DIK_M               0x32
    Key(51,  ",");   // #define DIK_COMMA           0x33
    Key(52,  ".");   // #define DIK_PERIOD          0x34    /* . on main keyboard */
    Key(53,  "/");   // #define DIK_SLASH           0x35    /* / on main keyboard */
    Key(59,  "F1");  // #define DIK_F1              0x3B
    Key(60,  "F2");  // #define DIK_F2              0x3C
    Key(61,  "F3");  // #define DIK_F3              0x3D
    Key(62,  "F4");  // #define DIK_F4              0x3E
    Key(63,  "F5");  // #define DIK_F5              0x3F
    Key(64,  "F6");  // #define DIK_F6              0x40
    Key(65,  "F7");  // #define DIK_F7              0x41
    Key(66,  "F8");  // #define DIK_F8              0x42
    Key(67,  "F9");  // #define DIK_F9              0x43
    Key(68,  "F10"); // #define DIK_F10             0x44
    Key(87,  "F11"); // #define DIK_F11             0x57
    Key(88,  "F12"); // #define DIK_F12             0x58

    Key(41,  "`");   // #define DIK_GRAVE           0x29    /* accent grave */
    Key(43,  "\\");  // #define DIK_BACKSLASH       0x2B

    // TODO no idea how to display this one; it's < (shift: >) on German keyboard and probably \ (shift: |) on UK keyboard; located next to left shift on some european layouts
	//Key(86, "&DIK_OEM_102&");	//#define DIK_OEM_102 0x56 /* < > | on UK/Germany keyboards */
}

