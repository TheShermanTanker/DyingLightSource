
sub None( float f_value = 0.0, string s_name = "none" )
{
    set( f_value )
    {
        description( s_name );
        varset()
        {
            LoopMode("smooth loop");
        }
    }
}

sub main()
{
    use None( f_value = 0.0, s_name = "Start" );
    use None( f_value = 1.0, s_name = "End" );
}
