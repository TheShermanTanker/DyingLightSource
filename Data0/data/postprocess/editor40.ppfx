import "main.ppfx"
import "std_first.ppfx"

sub main_ppfx(bool b_legacy_on = FALSE)
{
	int e_path = SM40;	

	use full_ppfx(
		e_path = e_path,
		b_opq_nrm_diff_on = FALSE,
		b_opq_clear_depth_on = TRUE,
		b_opq_clear_color_on = TRUE,
		b_heat_on = TRUE,
		b_sky_on = TRUE,
		b_rain_on = TRUE,
		b_editor_on = TRUE,
		b_editor_lighting_on = TRUE,
		b_shoots_on = FALSE,
		b_legacy_on = b_legacy_on
	);
		
}

sub ppfx()
{
	use main_ppfx();
}
