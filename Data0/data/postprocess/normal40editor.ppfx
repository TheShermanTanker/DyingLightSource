import "main.ppfx"

sub main_ppfx(bool b_legacy_on = FALSE)
{
	int e_path = SM40;

	use full_ppfx(
		e_path = e_path,
		b_opq_nrm_diff_on = FALSE,
		b_opq_clear_depth_on = TRUE,
		b_opq_clear_color_on = TRUE,
		b_heat_on = TRUE,
		b_sky_on = TRUE,
		b_rain_on = TRUE,
		b_editor_lighting_on = FALSE,
		b_editor_stencil_on = TRUE,
		b_shoots_on = TRUE,
		b_legacy_on = b_legacy_on,
		b_glow_on = TRUE, 
		b_blur_on = TRUE
	);										

	
}

sub ppfx()
{
	use main_ppfx();
}
